{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "destructuring": true,
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["import"],
  "rules": {
    "import/no-internal-modules": [
      "error",
      {
        "forbid": ["**/actions/*"]
      }
    ]
  }
}
